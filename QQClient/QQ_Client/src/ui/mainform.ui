#top_title QPushButton{
	border:none;
	width:20px;
	height:20px;
	border-radius:2px;
}

#top_title QPushButton:hover{
   border:1px solid rgb(13, 97, 137);
   
}

#top_title QPushButton#pushButton_minimize{
	background-image: url(:/sys/mainform_minimize.png);
}

#top_title QPushButton#pushButton_shutdown{
	background-image: url(:/sys/mainform_shutdown.png);
}


/*工具栏图标定义*/
#top_tools QPushButton{
	border:none;
	width:24px;
	height:20px;
