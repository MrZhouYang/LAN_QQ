	font-size:12pt;
}

#top_names #label_status{
	 
}

#top_names QLineEdit{
	color:#fff;
	font-size:10pt;
	background-color: rgba(255, 255, 255,0);
	border:none;
	border-radius:2px;
	 
}

#top_names QLineEdit:focus{
	background-color: rgb(255, 255, 255); 
	color:#000;
	border:1px solid rgb(213, 213, 213);
}

#top_names QLineEdit:hover{
