   <string notr="true">*{
	font: 75 9pt &quot;微软雅黑&quot;;
}

#color_main_widget{
	border-image: url(:/sys/back.png) 5;
	border-radius:2px;
}

#centralwidget{
	border-radius:2px;
	background-color: rgb(22, 154, 218);
}

/*标题栏*/
#color_main_top{
	
	border-top-left-radius:2px;
	border-top-right-radius:2px;
}

#color_main_top *{
	color:#fff;
	font-size:10pt;
}

/*缩小及关闭按钮*/
#color_main_top QPushButton{
	border:none;
	width:20px;
	height:20px;
	border-radius:2px;
}

#color_main_top QPushButton:hover{
   border:1px solid rgb(13, 97, 137);
   
}

#color_main_top QPushButton#pushButton_minimize{
	background-image: url(:/sys/mainform_minimize.png);
}

#color_main_top QPushButton#pushButton_shutdown{
	background-image: url(:/sys/mainform_shutdown.png);
}


#color_main_con{
	
	background-color: rgb(255, 255, 255);
	border-bottom-left-radius:2px;
	border-bottom-right-radius:2px;
}

#color_main_con #widget_tool{
	background-color: rgb(237, 238, 238);
	border-bottom:1px solid rgb(220,220,220);
}

#color_main_con #widget_colors{

}

#color_main_con #color_items QPushButton,#color_panle_pushButton{
	background-color: rgb(22, 154, 218);
	border:none;
}

#color_main_con #color_items QPushButton:hover,
#color_panle_pushButton:hover
{
	border:3px solid rgb(39,140,222);
}

#color_panle_pushButton{
	background-image: url(:/sys/color_panle.png);
	
}
